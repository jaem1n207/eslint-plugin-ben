{
  "name": "eslint-plugin-ben",
  "version": "1.1.0",
  "description": "ESLint rules for me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaem1n207/eslint-plugin-ben.git"
  },
  "homepage": "https://github.com/jaem1n207/eslint-plugin-ben#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ben"
  ],
  "author": "ben <roy.jm.lee@gmail.com>",
  "bugs": {
    "url": "https://github.com/jaem1n207/eslint-plugin-ben/issues"
  },
  "main": "build/index.js",
  "scripts": {
    "cz": "git-cz",
    "--------------------------------------------------": "",
    "generate-rule": "node tasks/rule/generate-rule.js",
    "-------------------------------------------------": "",
    "format": "prettier . --write",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator",
    "all-in-one": "npm-run-all --parallel update:eslint-docs format lint test",
    "------------------------------------------------": "",
    "build:dev": "node tasks/build.js --debug",
    "build": "node tasks/build.js",
    "prepublishOnly": "safe-publish-latest && npm run build",
    "prepublish": "not-in-publish || npm run prepublishOnly"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@rspack/cli": "^0.5.4",
    "@rspack/core": "^0.5.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "chalk": "^4.1.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "dedent": "^1.5.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "in-publish": "^2.0.1",
    "lefthook": "^1.6.1",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "prompts": "^2.4.2",
    "safe-publish-latest": "^2.0.0",
    "semantic-release": "^23.0.2"
  },
  "optionalDependencies": {
    "@rspack/binding-linux-x64-gnu": "^0.5.4"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "MIT"
}
